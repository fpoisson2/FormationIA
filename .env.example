# Renseignez votre clé OpenAI pour le backend
OPENAI_API_KEY=sk-votre-cle-ici

# Origine autorisée pour la couche CORS du backend
FRONTEND_ORIGIN=http://localhost:4173

# Jeton partagé entre frontend et backend pour sécuriser les appels API
API_AUTH_TOKEN=change-me

# URL du backend vue par le navigateur (utilisée par Vite au build)
VITE_API_BASE_URL=http://localhost:8000

# Identifiants de connexion pour l'interface (défaut : test / Telecom2025$)
VITE_LOGIN_USERNAME=test
VITE_LOGIN_PASSWORD=Telecom2025$

# Clé API partagée côté frontend (doit correspondre à API_AUTH_TOKEN)
VITE_API_AUTH_KEY=change-me
